	   "source": ["# coding=utf-8\n",
"from time import sleep\n",
"\n",
"from atlassian import Jira\n",
"\n",
"jira = Jira(url="http://localhost:8080/", username="jira-administrator", password="admin")\n",
"\n",
"jira.reindex()\n",
"\n",
"while not jira.reindex_status()["success"]:\n",
"    print("Still reindexing...")\n",
"    sleep(1)\n",
"\n",
"print("Done.")\n",]
	}
